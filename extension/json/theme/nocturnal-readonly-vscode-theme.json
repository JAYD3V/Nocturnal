        "name": "KEYWORDS (emphasized)",
        "scope":["storage.modifier", "keyword.control.export"],
        "settings": { "foreground": "#4090FC", "fontStyle": "italic" }
    },{
        "name": "KEYWORDS: CONTROL-FLOW",
        "scope":["keyword.control", "keyword.other"],
        "settings": { "foreground": "#4090FC" }
    },{
        "name": "OPERATORS",
        "scope":["keyword.operator"],
        "settings": { "foreground": "#DDBB55"}
    },{
        "name": "OPERATORS: OPERATOR-EXPRESSIONS)",
        "scope":["keyword.operator.new", "keyword.operator.expression"],
        "settings": { "foreground": "#DDBB55", "fontStyle": "italic"}
    },{
        "name": "OPERATORS: SMALL UNARY & ARITHMATIC",
        "scope":[
            "punctuation.accessor",
            "keyword.operator.accessor",
            "storage.type.function.arrow",
            "keyword.operator.increment",
            "keyword.operator.decrement"
