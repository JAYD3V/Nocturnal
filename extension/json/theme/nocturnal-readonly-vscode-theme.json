        "settings": { "foreground": "#DD705C"}
    },{
        "name": "LITERALS: NUMERIC",
        "scope":["constant.numeric"],
        "settings": { "foreground": "#BBAA44" }
    },{
        "name": "KEYWORDS: STORAGE DECLARATORS",
        "scope":["storage.type"],
        "settings": { "foreground": "#4090FC", "fontStyle": "bold"}
