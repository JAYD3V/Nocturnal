   /*



  ||========================================================================||
  ||"SAVE THESE PROPERTIES UNTIL THEY ARE STYLED IN THE "tokenColors" ARRAY"||
  ||==|| "punctuation.definition.directive",                      ||========||
      || "punctuation.definition.template-expression",            ||
      || "meta.preprocessor.include punctuation.definition.string"||
      ||==========================================================||

   *---------------------------------------------------------------------------*
                                SYNTAX HIGHLIGHTING
   *---------------------------------------------------------------------------*

      LITERAL BOOLEAN         -> #EEA0FF
      LITERAL NUMERIC         -> #BBAA44
      LITERAL STRINGS         -> #DD9977

      VARIABLES               -> #9DD3FF
      VARIABLE DECLARATIONS   -> #1FBFFF
      OBJECT-MEMBERS          -> #1FBFFF

      CLASS                   -> #30C888
      CLASS-MEMBERS           -> #1FBFFF

      FUNCTIONS               -> #88CB73
      FUNCTION-MEMBERS        -> #88CB73
      VARIABLE PARAMETERS     -> #DF84A8

      ANNOTATIONS (primitive) -> #0099AA (i)
   *****************************************************************************/

    "semanticTokenColors": {
      "class":                "#30C888",
      "interface":            "#30C888",
      "struct":               "#30C888",
      "namespace":            "#30C888",
      "function":             "#88CB73",
      "method":               "#88CB73",
      "variable":             "#9DD3FF",
      "property":             "#9DD3FF",
      "variable.declaration": "#1FBFFF",
      "property.declaration": "#1FBFFF",
      "parameter":            "#DF84A8",
      // "type": "#FFF0",
      // "typeParameter""#FFF0",
    },

    "tokenColors": [{
        "name": "COMMENTS",
        "scope":["comment"],
        "settings": { "foreground": "#387496" }
    },{
        "name": "STRING LITERALS",
        "scope":["string"],
        "settings": { "foreground": "#DD9977" }
    },{
        "name": "LITERAL",
        "scope": "constant.character",
        "settings": { "foreground": "#E56030", "fontStyle": "bold" }
    },{
        "name": "BOOLEAN LITERALS",
        "scope":"constant.language.boolean",
        "settings": { "foreground": "#F2AAEE"}
    },{
        "name": "LITERALS: UNDEFINED, NULL, & NaN",
        "scope":[
            "constant.language.undefined",
            "constant.language.null",
            "constant.language.NaN" // NaN is case sensitive
        ],
        "settings": { "foreground": "#DD705C"}
    },{
        "name": "LITERALS: NUMERIC",
        "scope":["constant.numeric"],
        "settings": { "foreground": "#BBAA44" }
    },{
        "name": "KEYWORDS: STORAGE DECLARATORS",
        "scope":["storage"],
        "settings": { "foreground": "#508FF0" }
    },{
        "name": "KEYWORDS (emphasized)",
        "scope":["storage.modifier", "keyword.control.export"],
        "settings": { "foreground": "#508FF0", "fontStyle": "italic" }
    },{
        "name": "KEYWORDS (emboldened)",
        "scope":["storage.modifier", "keyword.control.export"],
        "settings": { "foreground": "#508FF0", "fontStyle": "italic" }
    },{
        "name": "KEYWORDS: CONTROL-FLOW",
        "scope":["keyword.control"],
        "settings": { "foreground": "#508FF0" }
    },{
        "name": "KEYWORDS: OPERATORS",
        "scope":["keyword.operator", "keyword.other"],
        "settings": { "foreground": "#D0D0D0"}
    },{
        "name": "VARIABLES (generic)",
        "scope": ["variable"],
        "settings": { "foreground": "#9DD3FF" }
    },{
        "name": "VARIABLES: LANGUAGE DEFINED",
        "scope":["variable.language", "support.variable",],
        "settings": { "foreground": "#1FBFFF" }
    },{
        "name": "VARIABLES: THIS (lexically-scoped object)",
        "scope":["variable.language.this"],
        "settings": { "foreground": "#1FBFFF", "fontStyle": "italic" }
    },{
        "name": "VARIABLES: PARAMETERS",
        "scope": "meta.parameters variable.parameter",
        "settings": { "foreground": "#DF84A8" }
    },{
        "name": "VARIABLES: CLASS-MEMBERS & OBJECT-MEMBERS",
        "scope": [
            "variable.object.property",
            "meta.object.member",
            "meta.field.declaration",
            "meta.definition variable",
            "support.variable.property",
        ],
        "settings": { "foreground": "#1FBFFF" }
    },{
        "name": "TYPES & CLASSES",
        "scope": [
            "support.type",
            "support.class",
            "entity.name.class",
            "entity.other.inherited-class",
            "entity.name.type",
            "entity.name.namespace",
            "entity.name.interface",
            "entity.other.attribute",
            "entity.name.scope-resolution"
        ],
        "settings": { "foreground": "#30C888" }
    },{
        "name": "FUNCTIONS: ALL",
        "scope": "entity.name.function",
        "settings": { "foreground": "#8ED17E" }
    },{
        "name": "PRIMITIVE TYPE ANNOTATIONS (TS)",
        "scope": ["support.type.primitive"],
        "settings": { "foreground": "#0099AA", "fontStyle": "italic" }
    },

